
let root {}
let path []
let env_path ''
let env_prompt ''
let leds []

def init_files
 let root {}
 let path []
 let env_path '/env/path'
 let env_prompt '/env/prompt'

 / /env
 let _env_dir {}
 put $_env_dir 'path' '/programs'
 put $_env_dir 'prompt' '$p \'#\''
 put $root 'env' $_env_dir

 / init leds
 put $leds 0 0
 put $leds 1 0
 put $leds 2 0
 
 / set home dir
 psh $path 'home'
end

cal init_files
